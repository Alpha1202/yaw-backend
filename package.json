{
  "name": "yaw-backend",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "start:dev": "set DEBUG=App & nodemon --exec babel-node src/index.js",
    "start": "npm run build && node lib/index.js",
    "build": "babel src -d lib",
    "test": "cross-env NODE_ENV=test nyc mocha --timeout 20000  --require @babel/register  test/**.test.js --exit",
    "test:watch": "jest --watch --coverage --colors",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "migrate": "sequelize db:migrate",
    "undo:migrate": "sequelize db:migrate:undo:all",
    "seed": "sequelize db:seed:all"
  },
  "nyc": {
    "exclude": [
      "src/helpers/*.js",
      "src/index.js",
      "src/socialAuthConfig/SocialConfig.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alpha1202/yaw-backend.git"
  },
  "keywords": [],
