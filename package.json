    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/register": "^7.7.0",
    "coveralls": "^3.0.7",
    "cross-env": "^6.0.3",
    "debug": "^4.1.1",
    "eslint": "^6.6.0",
