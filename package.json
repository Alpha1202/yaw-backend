    "nodemon": "^1.19.4",
    "nyc": "^14.1.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "@babel/runtime": "^7.7.2",
    "@babel/runtime-corejs2": "^7.7.2",
    "@hapi/joi": "^16.1.7",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
